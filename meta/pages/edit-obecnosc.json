{
  "access": [
    "Nauczyciel"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "czy_byl",
                              "name": "czy_bylLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Obecność",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "czy_byl",
                              "triState": true,
                              "type": "checkbox",
                              "value": "${obecnosc.czy_byl}"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "uczen_id",
                              "name": "uczen_idLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Uczeń",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "count": "${getUczensForuczen_idCount}",
                              "data": "${getUczensForuczen_idResult}",
                              "emptyText": "Brak rekordów",
                              "events": {
                                "loadData": [
                                  {
                                    "name": "Baza.getUczens",
                                    "parameters": [
                                      {
                                        "name": "$top",
                                        "value": "${event.top}"
                                      },
                                      {
                                        "name": "$skip",
                                        "value": "${event.skip}"
                                      },
                                      {
                                        "name": "$orderby",
                                        "value": "${event.orderby}"
                                      },
                                      {
                                        "name": "$filter",
                                        "value": "${event.filter}"
                                      },
                                      {
                                        "name": "$count",
                                        "value": "true"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "getUczensForuczen_idResult",
                                        "type": "set",
                                        "value": "${result.value}"
                                      },
                                      {
                                        "name": "getUczensForuczen_idCount",
                                        "type": "set",
                                        "value": "result['@odata.count']"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "name": "uczen_id",
                              "pageSize": "${getUczensForuczen_idPageSize}",
                              "placeholder": "Wybierz ucznia",
                              "searchText": "Szukaj...",
                              "selectedValue": "${getByUczensForuczen_idResult}",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "adres",
                              "type": "dropdown-datagrid",
                              "value": "${obecnosc.uczen_id}",
                              "valueProperty": "uczen_id"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "data_opis_id",
                              "name": "data_opis_idLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Opis",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "count": "${getDataOpisFordata_opis_idCount}",
                              "data": "${getDataOpisFordata_opis_idResult}",
                              "emptyText": "Brak rekordów.",
                              "events": {
                                "loadData": [
                                  {
                                    "name": "Baza.getDataOpis",
                                    "parameters": [
                                      {
                                        "name": "$top",
                                        "value": "${event.top}"
                                      },
                                      {
                                        "name": "$skip",
                                        "value": "${event.skip}"
                                      },
                                      {
                                        "name": "$orderby",
                                        "value": "${event.orderby}"
                                      },
                                      {
                                        "name": "$filter",
                                        "value": "${event.filter}"
                                      },
                                      {
                                        "name": "$count",
                                        "value": "true"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "getDataOpisFordata_opis_idResult",
                                        "type": "set",
                                        "value": "${result.value}"
                                      },
                                      {
                                        "name": "getDataOpisFordata_opis_idCount",
                                        "type": "set",
                                        "value": "result['@odata.count']"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "name": "data_opis_id",
                              "pageSize": "${getDataOpisFordata_opis_idPageSize}",
                              "placeholder": "Wybierz opis",
                              "searchText": "Szukaj...",
                              "selectedValue": "${getByDataOpisFordata_opis_idResult}",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "opis",
                              "type": "dropdown-datagrid",
                              "value": "${obecnosc.data_opis_id}",
                              "valueProperty": "data_opis_id"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Zapisz",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "back"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Anuluj",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Obecnosc",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.updateObecnosc",
                        "parameters": [
                          {
                            "name": "ocena_id",
                            "value": "${parameters.ocena_id}"
                          },
                          {
                            "name": "Obecnosc",
                            "value": "${event}"
                          },
                          {
                            "name": "$expand",
                            "value": "Uczen,DataOpi"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "form0",
                  "type": "template-form"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "error": [],
        "name": "Baza.getObecnoscByocena_id",
        "parameters": [
          {
            "name": "ocena_id",
            "value": "${parameters.ocena_id}"
          }
        ],
        "then": [
          {
            "name": "obecnosc",
            "type": "set",
            "value": "${result}"
          },
          {
            "condition": "this.obecnosc.uczen_id != null",
            "name": "Baza.getUczenByuczen_id",
            "parameters": [
              {
                "name": "uczen_id",
                "value": "${this.obecnosc.uczen_id}"
              }
            ],
            "then": [
              {
                "name": "getByUczensForuczen_idResult",
                "type": "set",
                "value": "${result}"
              }
            ],
            "type": "invoke"
          },
          {
            "condition": "this.obecnosc.data_opis_id != null",
            "name": "Baza.getDataOpiBydata_opis_id",
            "parameters": [
              {
                "name": "data_opis_id",
                "value": "${this.obecnosc.data_opis_id}"
              }
            ],
            "then": [
              {
                "name": "getByDataOpisFordata_opis_idResult",
                "type": "set",
                "value": "${result}"
              }
            ],
            "type": "invoke"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "getUczensForuczen_idPageSize",
        "type": "set",
        "value": "10"
      },
      {
        "name": "Baza.getUczens",
        "parameters": [
          {
            "name": "$top",
            "value": "${getUczensForuczen_idPageSize}"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getUczensForuczen_idResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getUczensForuczen_idCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "getDataOpisFordata_opis_idPageSize",
        "type": "set",
        "value": "10"
      },
      {
        "name": "Baza.getDataOpis",
        "parameters": [
          {
            "name": "$top",
            "value": "${getDataOpisFordata_opis_idPageSize}"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getDataOpisFordata_opis_idResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getDataOpisFordata_opis_idCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Edit Obecnosc"
}