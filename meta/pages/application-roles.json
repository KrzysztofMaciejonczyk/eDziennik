{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Role",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Dodaj rolę",
                  "allowAdd": true,
                  "allowDelete": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "property": "Name",
                      "title": "Nazwa"
                    }
                  ],
                  "confirmDeleteCancelText": "Anuluj",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Na pewno chcesz usunąć ten rekord?",
                  "confirmDeleteTitle": "Potwierdź",
                  "containsText": "Contains",
                  "data": "${getRolesResult}",
                  "emptyText": "Brak rekordów.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-application-role",
                        "type": "popup"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "${result.error.message}",
                            "severity": "error",
                            "summary": "Cannot delete role",
                            "type": "notify"
                          }
                        ],
                        "name": "Security.deleteRole",
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${event.Id}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Role \"${event.Name}\" has been deleted.",
                            "severity": "info",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "Security.getRoles",
        "then": [
          {
            "name": "getRolesResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Application Roles"
}