{
  "access": [
    "Nauczyciel"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Obecności",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Dodaj obecność",
                  "allowAdd": true,
                  "allowDelete": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "property": "czy_byl",
                      "title": "Obecność",
                      "type": "boolean"
                    },
                    {
                      "filterProperty": "Uczen.adres",
                      "property": "uczen_id",
                      "sortProperty": "Uczen.adres",
                      "template": "${data.Uczen?.adres}",
                      "title": "Uczeń_ID",
                      "type": "string"
                    },
                    {
                      "filterProperty": "DataOpi.opis",
                      "property": "data_opis_id",
                      "sortProperty": "DataOpi.opis",
                      "template": "${data.DataOpi?.opis}",
                      "title": "Opis_ID",
                      "type": "string"
                    },
                    {
                      "property": "ocena_id",
                      "title": "Ocena_ID",
                      "type": "integer"
                    }
                  ],
                  "confirmDelete": true,
                  "confirmDeleteCancelText": "Anuluj",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Na pewno chcesz usunąć ten rekord?",
                  "confirmDeleteTitle": "Potwierdź",
                  "containsText": "Contains",
                  "count": "${getObecnoscsCount}",
                  "data": "${getObecnoscsResult}",
                  "emptyText": "Brak wyników.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-obecnosc",
                        "type": "popup"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete Obecnosc",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.deleteObecnosc",
                        "parameters": [
                          {
                            "name": "ocena_id",
                            "value": "${event.ocena_id}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Obecnosc deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load Obecnoscs",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.getObecnoscs",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Uczen,DataOpi"
                          }
                        ],
                        "then": [
                          {
                            "name": "getObecnoscsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getObecnoscsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "ocena_id",
                            "value": "${event.ocena_id}"
                          }
                        ],
                        "path": "edit-obecnosc",
                        "type": "popup"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main",
  "name": "Obecnosc"
}