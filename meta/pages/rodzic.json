{
  "access": [
    "Admin"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Rodzice",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Dodaj rodzica",
                  "allowAdd": true,
                  "allowDelete": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "filterProperty": "Osoba.imie",
                      "property": "osoba_id",
                      "sortProperty": "Osoba.imie",
                      "template": "",
                      "title": "Osoba_ID",
                      "type": "string"
                    },
                    {
                      "property": "rodzic_id",
                      "title": "Rodzic_ID",
                      "type": "integer"
                    }
                  ],
                  "confirmDelete": true,
                  "confirmDeleteCancelText": "Anuluj",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Na pewno chcesz usunąć ten rekord?",
                  "confirmDeleteTitle": "Potwierdź",
                  "containsText": "Contains",
                  "count": "${getRodzicsCount}",
                  "data": "${getRodzicsResult}",
                  "emptyText": "Brak wyników.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-rodzic",
                        "type": "popup"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete Rodzic",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.deleteRodzic",
                        "parameters": [
                          {
                            "name": "rodzic_id",
                            "value": "${event.rodzic_id}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Rodzic deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load Rodzics",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.getRodzics",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Uczens,Osoba"
                          }
                        ],
                        "then": [
                          {
                            "name": "getRodzicsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getRodzicsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "rodzic_id",
                            "value": "${event.rodzic_id}"
                          }
                        ],
                        "path": "edit-rodzic",
                        "type": "popup"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main",
  "name": "Rodzic"
}