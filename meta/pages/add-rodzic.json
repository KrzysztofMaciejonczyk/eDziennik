{
  "access": [
    "Authenticated",
    "Rodzic"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "osoba_id",
                              "name": "osoba_idLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Osoba",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "count": "${getOsobasForosoba_idCount}",
                              "data": "${getOsobasForosoba_idResult}",
                              "emptyText": "Brak wyników do wyświetlenia.",
                              "events": {
                                "loadData": [
                                  {
                                    "name": "Baza.getOsobas",
                                    "parameters": [
                                      {
                                        "name": "$top",
                                        "value": "${event.top}"
                                      },
                                      {
                                        "name": "$skip",
                                        "value": "${event.skip}"
                                      },
                                      {
                                        "name": "$orderby",
                                        "value": "${event.orderby}"
                                      },
                                      {
                                        "name": "$filter",
                                        "value": "${event.filter}"
                                      },
                                      {
                                        "name": "$count",
                                        "value": "true"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "getOsobasForosoba_idResult",
                                        "type": "set",
                                        "value": "${result.value}"
                                      },
                                      {
                                        "name": "getOsobasForosoba_idCount",
                                        "type": "set",
                                        "value": "result['@odata.count']"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "name": "osoba_id",
                              "pageSize": "${getOsobasForosoba_idPageSize}",
                              "placeholder": "Wybierz osobę",
                              "searchText": "Szukaj...",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "osoba_id",
                              "type": "dropdown-datagrid",
                              "valueProperty": "osoba_id"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Zapisz",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "back"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Anuluj",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Rodzic!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.createRodzic",
                        "parameters": [
                          {
                            "name": "Rodzic",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "form0",
                  "type": "template-form"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "getOsobasForosoba_idPageSize",
        "type": "set",
        "value": "10"
      },
      {
        "name": "Baza.getOsobas",
        "parameters": [
          {
            "name": "$top",
            "value": "${getOsobasForosoba_idPageSize}"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getOsobasForosoba_idResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getOsobasForosoba_idCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Rodzic"
}