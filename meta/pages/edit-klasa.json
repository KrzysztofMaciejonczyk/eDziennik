{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "nauczyciel_id",
                              "name": "nauczyciel_idLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Nauczyciel",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getNauczycielsResult}",
                              "emptyFilterText": "Brak wynik√≥w",
                              "multipleItemsSelectedText": "{0} wybranych",
                              "name": "nauczyciel_id",
                              "placeholder": "Wybierz nauczyciela",
                              "style": {
                                "width": "100%"
                              },
                              "textProperty": "nauczyciel_id",
                              "type": "dropdown",
                              "value": "${klasa.nauczyciel_id}",
                              "valueProperty": "nauczyciel_id"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "nazwa",
                              "name": "nazwaLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Nazwa",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 2,
                              "name": "nazwa",
                              "style": {
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${klasa.nazwa}"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Zapisz",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "back"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Anuluj",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Klasa",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.updateKlasa",
                        "parameters": [
                          {
                            "name": "klasa_id",
                            "value": "${parameters.klasa_id}"
                          },
                          {
                            "name": "Klasa",
                            "value": "${event}"
                          },
                          {
                            "name": "$expand",
                            "value": "Nauczyciel"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "form0",
                  "type": "template-form"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "error": [],
        "name": "Baza.getKlasaByklasa_id",
        "parameters": [
          {
            "name": "klasa_id",
            "value": "${parameters.klasa_id}"
          }
        ],
        "then": [
          {
            "name": "klasa",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Baza.getNauczyciels",
        "parameters": [],
        "then": [
          {
            "name": "getNauczycielsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Edit Klasa"
}