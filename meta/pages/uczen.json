{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Uczen"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "name": "grid0",
                  "allowDelete": true,
                  "confirmDelete": true,
                  "allowAdd": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "count": "${getUczensCount}",
                  "data": "${getUczensResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "Baza.getUczens",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Uwagas,Ocenas,Obecnoscs,Osoba,Rodzic,Klasa"
                          }
                        ],
                        "then": [
                          {
                            "name": "getUczensResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getUczensCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke",
                        "error": [
                          {
                            "detail": "Unable to load Uczens",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ]
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete Uczen",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Baza.deleteUczen",
                        "parameters": [
                          {
                            "name": "uczen_id",
                            "value": "${event.uczen_id}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Uczen deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "add": [
                      {
                        "path": "add-uczen",
                        "type": "popup"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "uczen_id",
                            "value": "${event.uczen_id}"
                          }
                        ],
                        "path": "edit-uczen",
                        "type": "popup"
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "data_urodzenia",
                      "formatString": "medium",
                      "title": "Data Urodzenia",
                      "type": "string",
                      "format": "date-time"
                    },
                    {
                      "property": "osoba_id",
                      "sortProperty": "Osoba.imie",
                      "filterProperty": "Osoba.imie",
                      "template": "${data.Osoba?.imie}",
                      "title": "Osoba",
                      "type": "string"
                    },
                    {
                      "property": "rodzic_id",
                      "sortProperty": "Rodzic.rodzic_id",
                      "filterProperty": "Rodzic.rodzic_id",
                      "template": "${data.Rodzic?.rodzic_id}",
                      "title": "Rodzic",
                      "type": "integer"
                    },
                    {
                      "property": "klasa_id",
                      "sortProperty": "Klasa.nazwa",
                      "filterProperty": "Klasa.nazwa",
                      "template": "${data.Klasa?.nazwa}",
                      "title": "Klasa",
                      "type": "string"
                    },
                    {
                      "property": "uczen_id",
                      "title": "Uczen Id",
                      "type": "integer"
                    },
                    {
                      "property": "adres",
                      "title": "Adres",
                      "type": "string"
                    }
                  ],
                  "type": "grid",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteCancelText": "Cancel",
                  "emptyText": "No records to display.",
                  "addText": "Add",
                  "clearFilterText": "Clear",
                  "applyFilterText": "Apply",
                  "equalsText": "Equals",
                  "notEqualsText": "Not equals",
                  "lessThanText": "Less than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "greaterThanText": "Greater than",
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "endsWithText": "Ends with",
                  "containsText": "Contains",
                  "startsWithText": "Starts with"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "type": "execute",
        "code": "this.grid0.load();"
      }
    ]
  },
  "name": "Uczen",
  "access": [
    "Admin",
    "Authenticated"
  ],
  "layout": "Main"
}